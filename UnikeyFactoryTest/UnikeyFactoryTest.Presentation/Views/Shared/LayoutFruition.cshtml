<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <script src="~/Scripts/jquery-3.4.1.js"></script>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/css/styles.css")
    @Scripts.Render("~/Content/Scripts/scripts.js")
</head>
<body>
    <div id="renderDiv" class="container body-content">
        <div id="cover-spin"></div>
        <div id="mySidenav" class="sidenav">
            <a id="homeLogo"><img id="unikeyLogo" src="~/Content/ICONS/unikeyLogo.png" alt="UnikeyLogo"></a>
        </div>
        @RenderBody()
        <br />
        <hr />
    </div>

    @*@Scripts.Render("~/bundles/jquery")*@
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>